let num = 13;
if (num % 2 === 0) {
    console.log(num + " is Even");
} else {
    console.log(num + " is Odd");
}


let languages = ["HTML", "CSS", "JavaScript"];
console.log("Languages:");
languages.forEach(item => console.log(item));
